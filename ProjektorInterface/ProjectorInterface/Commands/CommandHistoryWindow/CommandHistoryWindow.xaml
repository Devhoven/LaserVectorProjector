<UserControl x:Class="ProjectorInterface.Commands.CommandHistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectorInterface.Commands"
        mc:Ignorable="d">
    <DockPanel Width="200" Height="300">
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="Undo" Click="UndoClick"/>
            <MenuItem Header="Redo" Click="RedoClick"/>
        </Menu>
        <local:AutoScrollViewer DockPanel.Dock="Bottom">
            <StackPanel x:Name="HistoryPanel" Background="White">
            </StackPanel>
        </local:AutoScrollViewer>
    </DockPanel>
</UserControl>
